{
  "name": "init",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.3.2",
    "@material-ui/icons": "^4.2.1",
    "connect-multiparty": "^2.2.0",
    "formidable": "^1.2.1",
    "parcel-bundler": "^1.12.3",
    "react": "^16.9.0",
    "react-butterfiles": "^1.3.3",
    "react-dom": "^0.14.0 || ^15.0.0 || ^16",
    "react-markdown": "^4.2.2",
    "react-modal": "^3.10.1",
    "react-scripts": "3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "babel-preset-react-app": "7",
    "prettier-eslint": "^8.8.2",
    "typescript": "^3.5.1"
  },
  "scripts": {
    "start": "./node_modules/.bin/parcel watch src/index.js -d public/build",
    "build": "./node_modules/.bin/parcel build src/index.js -d public/build",
    "local": "react-scripts start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry"
        }
      ],
      [
        "babel-preset-react-app",
        {
          "absoluteRuntime": false
        }
      ]
    ],
    "plugins": [
      "@babel/proposal-class-properties"
    ]
  }
}
